{
	"microservice.metadata":{
      "service.id": "Test.Flow",
      "component.id":"Relay",
      "errorPayload": {
        "queue":"Test.Flow.Relay.error",
        "ack": true
      },
      "errorMessage": {
        "queue":"Test.Flow.Relay.error.message",
        "ack": true
      }
    },
    "broker":{
		"host":"localhost",
		"port":6379,
        "retry.attempts": null,
		"retry.interval": 10
	},
	"publishers":{
		"main.publisher":{
			"queue":"relay.downstream",
			"ack":true
		}
	},
	"subscribers":{
		"main.subscriber":{
			"queue":"upstream.relay",
			"type":"persistent"
		}
	}
}


